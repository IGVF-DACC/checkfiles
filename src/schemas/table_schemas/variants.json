{
    "fields": [
        {
            "name": "Genome reference",
            "type": "string",
            "description": "the reference genome being utilized",
            "constraints": {
                "required": true,
                "enum": [
                    "GRCh38",
                    "GRCm39"
                ]
            }
        },
        {
            "name": "Chromosome",
            "type": "string",
            "description": "RefSeq ID of the variant's chromosom",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "Position",
            "type": "string",
            "description": "1-based position of the starting position of variant",
            "constraints": {
                "minimum": 1,
                "required": true
            }
        },
        {
            "name": "variant_start",
            "type": "integer",
            "description": "0-based position of the starting position of variant",
            "constraints": {
                "minimum": 0,
                "required": true
            }
        },
        {
            "name": "variant_end",
            "type": "integer",
            "description": "0-based position of the end position of variant",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "ref_allele",
            "type": "string",
            "description": "reference sequence of allele ",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "alt_allele",
            "type": "string",
            "description": "alternative sequence of allele ",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "rsID",
            "type": "string",
            "description": "rsID of variant"
        },
        {
            "name": "variant_type",
            "type": "string",
            "description": "describing the mutation type",
            "constraints": {
                "enum": [
                    "SNV",
                    "MNV",
                    "INS",
                    "DEL",
                    "INDEL"
                ]
            }
        },
        {
            "name": "protein_change",
            "type": "string",
            "description": "description of variant's effect on corresponding protein"
        },
        {
            "name": "associated_gene",
            "type": "string",
            "description": "HGNC ID of associated gene"
        },
        {
            "name": "associated_trait",
            "type": "string",
            "description": "phenotypic trait associated with variant. Should map to trait described in an ontology database (DOID, EFO, HP, MONDO, OBA)"
        },
        {
            "name": "OMIM_ref",
            "type": "string",
            "description": "ID referencing an associated entry in the OMIM compendium"
        },
        {
            "name": "pubmed_ref",
            "type": "string",
            "description": "ID referencing an associated paper in the PubMed catalog"
        },
        {
            "name": "annotation",
            "type": "string",
            "description": "flexible tag used to store any additional details associated with the variant"
        }
    ],
    "missingValues": [
        ""
    ]
}
