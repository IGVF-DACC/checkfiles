{
    "fields": [
        {
            "name": "genome_reference",
            "type": "string",
            "description": "the reference genome being utilized",
            "constraints": {
                "required": true,
                "enum": [
                    "GRCh38",
                    "GRCm39"
                ]
            }
        },
        {
            "name": "chromosome",
            "type": "string",
            "description": "RefSeq ID of the variant's chromosom",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "position",
            "type": "integer",
            "description": "1-based position of the starting position of variant",
            "constraints": {
                "minimum": 1,
                "required": true
            }
        },
        {
            "name": "reference_allele",
            "type": "string",
            "description": "reference sequence of allele",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "alternative_allele",
            "type": "string",
            "description": "alternative sequence of allele, could be “-” for deletion",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "rsID",
            "type": "string",
            "description": "rsID of variant"
        },
        {
            "name": "variant_type",
            "type": "string",
            "description": "describing the mutation type",
            "constraints": {
                "enum": [
                    "SNV",
                    "MNV",
                    "INS",
                    "DEL",
                    "INDEL"
                ]
            }
        },
        {
            "name": "associated_protein_change",
            "type": "string",
            "description": "description of variant's effect on corresponding protein, ex. nonsense mutation"
        },
        {
            "name": "associated_gene",
            "type": "string",
            "description": "HGNC ID of associated gene"
        },
        {
            "name": "associated_trait",
            "type": "string",
            "description": "phenotypic trait associated with variant. Should map to trait described in an ontology database (DOID, EFO, HP, MONDO, OBA)"
        },
        {
            "name": "OMIM",
            "type": "string",
            "description": "ID referencing an associated entry in the OMIM compendium"
        },
        {
            "name": "tissues_of_interest",
            "type": "string"
        },
        {
            "name": "cell_types_of_interest",
            "type": "string"
        },
        {
            "name": "experiments",
            "type": "string",
            "description": "experiments that will be conducted"
        },
        {
            "name": "pubmed_ref",
            "type": "string",
            "description": "ID referencing an associated paper in the PubMed catalog, example: PMID:123456, doi :/10.10.20.30"
        },
        {
            "name": "other",
            "type": "string",
            "description": "free text used to store any additional details associated with the variant"
        }
    ],
    "missingValues": [
        ""
    ]
}
